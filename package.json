{
  "name": "vite-handlebars",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build --emptyOutDir",
    "start": "npm run build && node ./server.js",
    "preview": "vite preview",
    "test": "mocha",
    "eslint": "eslint ./src/*/*.ts",
    "stylelint": "stylelint  '**/*{.css,.scss}'",
