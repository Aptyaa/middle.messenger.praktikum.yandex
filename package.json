    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "npm run eslint --fix"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.4.6",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "esm-loader-css": "^1.0.6",
    "esm-loader-typescript": "^1.0.6",
    "esmock": "^2.6.4",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "node-esm-loader": "^0.3.0",
    "sass": "^1.64.2",
    "sinon": "^17.0.1",
    "stylelint": "^16.1.0",
