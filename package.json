    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-handlebars": "^1.6.0",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "nanoid": "^5.0.4"
  },
  "engines": {
    "node": ">= 12.0.0"
