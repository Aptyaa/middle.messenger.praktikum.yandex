{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ESNext", "DOM", "ES5", "ES6", "DOM.Iterable"],
    "moduleResolution": "node",
    "noEmit": true
  }
}
