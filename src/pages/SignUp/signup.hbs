<div class='signup-page'>
  {{#Card pageName=this.pageName ref='formInfo'}}
    {{#each inputs}}
      {{{Input type=this.type placeholder=this.placeholder name=this.name}}}
    {{/each}}
    <div class='buttons-container_reg'>
      {{{Button
        label='Зарегистрироваться'
        onClick=this.button.onSubmit
      }}}
      {{{Ref onClick=this.ref.onClick border_color='white' ref_name='Войти'}}}
    </div>

  {{/Card}}
</div>
